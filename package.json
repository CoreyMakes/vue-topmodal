{
  "name": "vue-topmodal",
  "license": "MIT",
  "version": "0.2.1",
  "private": false,
  "main": "./dist/TopModal.common.js",
  "unpkg": "./dist/TopModal.umd.min.js",
  "homepage": "https://github.com/adileo/vue-topmodal",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name TopModal ./src/components/TopModal.vue"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.1",
    "vue": "^2.5.22",
    "vue-mq": "^1.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "vue.js",
    "modal",
    "alert",
    "overlay",
    "dialog",
    "lightbox",
    "popup"
  ]
}
